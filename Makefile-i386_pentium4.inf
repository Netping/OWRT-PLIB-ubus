SECTION="NetPing modules"
CATEGORY="Base"
TITLE="EPIC OWRT-PLIB-ubus"

PKG_NAME="OWRT-PLIB-ubus"
PKG_VERSION="0.1.1-1"
PKG_RELEASE=2

LIB_DIR=/usr/lib/python3.7/lib-dynload
LIB=ubus.cpython-37-i386_pentium4.so

.PHONY: all install

all: install

install:
        mkdir -p $(LIB_DIR)
        cp $(LIB) $(LIB_DIR)/ubus.cpython-37.so

clean:
        rm -f $(LIB_DIR)/ubus.cpython-37.so
